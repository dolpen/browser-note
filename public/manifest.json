{
  "name": "BrowserNote",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "URLベースのノート",
  "browser_action": {
    "default_title": "BrowserNote",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "unlimitedStorage"
  ]
}
